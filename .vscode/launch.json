{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core NUnit",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "$Qa_Automation/Selenium_C#.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "Selenium_C#/Views"
            }
        }
    ]
}
